 50%;\n    transform: translateX(-50%) translateY(50%);\n    content: \" \";\n}\n\n/* APPLY BTN */\n/*  */\n.apply-btn {\n    width: 320px;\n    height: 32px;\n    border: none;\n    outline: transparent;\n    color: #1e2761;\n    cursor: pointer;\n    border-radius: 80px;\n    margin-top: 30px;\n}\n\n/* MENU */\n/*  */\n.menu {\n    width: 96px;\n    min-height: 48px;\n}\n\n.menu ul{\n    background-color: #458ec2a6;\n    border-radius: 0 5px 5px 5px;\n    margin-top: 3px;\n    list-style: none;\n    box-sizing: content-box;\n    display: none;\n}\n\n.menu li {\n    position: relative;\n}\n\n.menu li button {\n    background: none;\n    outline: none;\n    border: none;\n    color: inherit;\n    min-width: 100%;\n    height: 100%;\n    text-align: start;\n    padding: 5px;\n    white-space: nowrap;\n}\n\n.menu:hover > ul {\n    display: block;\n}\n\n/* SUB MENU */\n/*  */\n.sub-menu ul{\n    position: absolute;\n    top: 0;\n    left: 100%;\n    min-width: 100%;\n    margin-left: 1px;\n}\n\n.sub-menu:hover > ul {\n    display: block;\n}\n\n/* MENU HOVER BTN */\n/*  */\n.menu-hover {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    width: 100%;\n    height: 28px;\n    border: none;\n    border-radius: 4px;\n    text-align: start;\n    color: #1e2761;\n    padding: 5px;\n    outline: none;\n}\n\n.menu-hover::after {\n    width: 10px;\n    height: 10px;\n    margin-bottom: 4px;\n    margin-right: 4px;\n    content: \"\";\n    display: block;\n    border: solid #1e2761;\n    border-width: 0 2.4px 2.4px 0;\n    transform: rotate(45deg);\n}\n\n.level {\n    cursor: pointer;\n}\n\n/* UPLOAD */\n/*  */\ninput[type=\"file\"]::-ms-browse {\n    font: inherit;\n    font-style: initial;\n    width: 128px;\n    height: 32px;\n    border: none;\n    outline: transparent;\n    color: #1e2761;\n    cursor: pointer;\n    border-radius: 80px;\n    margin-right: 8px;\n}\n\ninput[type=\"file\"]::-webkit-file-upload-button {\n    font: inherit;\n    font-style: initial;\n    width: 128px;\n    height: 32px;\n    border: none;\n    outline: transparent;\n    color: #1e2761;\n    cursor: pointer;\n    border-radius: 80px;\n    margin-right: 8px;\n}\n\n#export {\n    align-self: center;\n}\n\n/* EASTER EGG */\n/*  */\n#easter-egg {\n    align-self: flex-start;\n    margin-left: auto;\n    border: none;\n    border-top: 32px solid whitesmoke;\n    border-left: 32px solid #7a2048;\n    outline: none;\n    cursor: pointer;\n}\n\n/* HOVER */\n/*  */\n.add-btn:hover, .minus-btn:hover, .apply-btn:hover, .menu-hover:hover, .menu li button:hover, input[type=\"file\"]::-webkit-file-upload-button:hover{\n    background-color: #408ec6;\n    transition: 0.3s;\n}\n\ninput[type=\"file\"]::-ms-browse:hover {\n    background-color: #408ec6;\n    transition: 0.3s;\n}\n\n#easter-egg:hover {\n    border-top: 32px solid #408ec6;\n    transition: 0.3s;\n}\n"},"lineCount":222}},"error":null,"hash":"fa9ee7a31af6daf414b23ca35e339ded","cacheData":{"env":{}}}